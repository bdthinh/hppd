<!DOCTYPE html>
<html>
  <head>
    <title>Eaststudio</title>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <%= render 'layouts/flash' %>
    <div class="container">
      <div class="row">
        <div class="col-md-6 col-md-offset-3"><%= yield %></div>
      </div>
    </div>
    <%= render 'layouts/footer' %>
  </body>
</html>